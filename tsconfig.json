    "compilerOptions": {
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "downlevelIteration": true,
